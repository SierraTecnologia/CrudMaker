{
    "name": "sierratecnologia/crudmaker",
    "description": "An incredibly powerful and some say magical CRUD maker for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "ricardo@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^2.5",
        "illuminate/support": "^7.0 || ^8.0",
        "sierratecnologia/formmaker": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "mockery/mockery": ">=1.0",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "SierraTecnologia\\CrudMaker\\": "src/"
        },
        "files": [
            "src/Helpers/namespace.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SierraTecnologia\\CrudMaker\\CrudMakerProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
